#set editing-mode emacs
"\e[3~":delete-char
"\C-?": backward-delete-char
"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[7~": beginning-of-line
"\e[8~": end-of-line
"\eOH": beginning-of-line
"\eOF": end-of-line
"\e[H": beginning-of-line
"\e[F": end-of-line
"\e[1;5C": forward-word        # ctrl right
"\e[1;5D": backward-word       # ctrl left